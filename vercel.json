{
  "version": 2,
  "builds": [],
  "routes": [
    {
      "src": "/(.*\\.(css|js|json|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
      "headers": { "cache-control": "public, max-age=31536000" },
      "dest": "/$1"
    },
    { "src": "/pages/(.*)", "dest": "/pages/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
